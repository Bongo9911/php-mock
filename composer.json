{
    "name": "php-mock/php-mock",
    "type": "library",
    "description": "PHP-Mock can mock built-in PHP functions (e.g. time()). PHP-Mock relies on PHP's namespace fallback policy. No further extension is needed.",
    "keywords": ["mock", "stub", "test double", "function", "test", "TDD", "BDD"],
    "homepage": "https://github.com/php-mock/php-mock",
    "license": "WTFPL",
    "authors": [
        {
            "name": "Markus Malkusch",
            "email": "markus@malkusch.de",
            "homepage": "http://markus.malkusch.de",
            "role": "Developer"
        }
    ],
    "autoload": {
        "files": ["autoload.php"],
        "psr-4": {
            "phpmock\\": ["classes/", "tests/"]
        }
    },
    "autoload-dev": {
        "files": ["tests/autoload.php"]
    },
    "require": {
        "php": "^5.6 || ^7.0 || ^8.0",
        "phpunit/php-text-template": "^3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0.x-dev",
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/php-code-coverage": "^10.0.x-dev",
        "phpunit/php-file-iterator": "^4.0.x-dev",
        "phpunit/php-invoker": "4.0.x-dev",
        "phpunit/php-timer": "6.0.x-dev",
        "sebastian/cli-parser": "^2.0.x-dev",
        "sebastian/code-unit": "^2.0.x-dev",
        "sebastian/comparator": "^5.0.x-dev",
        "sebastian/diff": "^5.0.x-dev",
        "sebastian/environment": "^6.0.x-dev",
        "sebastian/exporter": "^5.0.x-dev",
        "sebastian/global-state": "^6.0.x-dev",
        "sebastian/object-enumerator": "^5.0.x-dev",
        "sebastian/type": "4.0.x-dev",
        "sebastian/version": "^4.0.x-dev",
        "sebastian/code-unit-reverse-lookup": "3.0.x-dev",
        "sebastian/complexity": "^3.0.x-dev",
        "sebastian/lines-of-code": "^2.0.x-dev",
        "theseer/tokenizer": "^1.2.0",
        "sebastian/recursion-context": "^5.0.x-dev",
        "sebastian/object-reflector": "^3.0.x-dev"
    },
    "replace": {
        "malkusch/php-mock": "*"
    },
    "suggest": {
	    "php-mock/php-mock-phpunit": "Allows integration into PHPUnit testcase with the trait PHPMock."
    },
    "archive": {
        "exclude": ["/tests"]
    }
}
